{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "mongo": true,
    "jest": true
  },
  "parserOptions": { "sourceType": "module" },
  "extends": "eslint:recommended",
  "rules": {
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "no-console": 0,
    "no-unused-vars": "warn"
  }
}
